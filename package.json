{
  "name": "dora",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dora-js/dora"
  },
  "homepage": "https://github.com/dora-js/dora",
  "author": "chencheng <sorrycc@gmail.com>",
  "license": "MIT",
  "main": "./lib",
  "bin": {
    "dora": "./bin/dora"
  },
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore __tests__",
    "test": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --no-timeouts",
    "debug": "./node_modules/.bin/mocha --require babel-core/register --require babel-polyfill --no-timeouts",
    "lint": "eslint --ext .js src",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "async": "^1.5.0",
    "babel-polyfill": "~6.6.0",
    "co": "^4.6.0",
    "commander": "~2.9.0",
    "is-generator-fn": "^1.0.0",
    "is-plain-object": "~2.0.1",
    "koa": "~1.2.0",
    "koa-serve-index": "~1.1.1",
    "koa-static": "^2.0.0",
    "loader-utils": "~0.2.11",
    "resolve": "~1.1.6",
    "spm-log": "^0.1.3"
  },
  "devDependencies": {
    "babel-cli": "~6.6.0",
    "babel-core": "~6.6.0",
    "babel-istanbul": "^0.6.0",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "coveralls": "~2.11.4",
    "eslint": "~2.3.0",
    "eslint-config-airbnb": "~3.1.0",
    "expect": "^1.13.4",
    "mocha": "^2.3.4",
    "pre-commit": "~1.1.2",
    "request": "^2.67.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "files": [
    "bin",
    "lib",
    "package.json",
    "README.md"
  ]
}
